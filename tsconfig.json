{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "ESNext"],
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "strict": true,
    "typeRoots": ["./node_modules/@types", "./src/@types"],
  },
  // "include": ["src"]
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
